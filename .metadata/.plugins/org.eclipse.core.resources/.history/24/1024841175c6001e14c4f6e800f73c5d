		StockCars stock=new StockCars();
		return stock;
	}
	
	@PostMapping("/postCars")
	public ResponseEntity PostCars(@RequestBody List<StockCars> cars) {
	List<CarsEntity> carsList=new ArrayList();
	
	}

}
